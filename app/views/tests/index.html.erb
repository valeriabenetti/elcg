<% if @tests.empty? %>
  <%= render 'empty_tests' %>
<% else %>
  <div id="main">
    <h1> Test Catalog</h1>
    <div class='row'>
      <div class="col-sm-12">
        <div id="test-list">
          <div class="list-group tests-list-group">
            <%= render @tests %>
          </div>
        </div>
        <%= paginate @tests %>
      </div>
    </div>
  </div>
<%end%>
