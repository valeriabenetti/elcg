<div id="search_sidebar" class="column col-md-2">
  <h2>Search Database</h2>
  <div id="search_form">
    <%= form_tag(tests_path, method: "GET", class: "navbar-form navbar-right search-form") do%>
      <div id="search_text" class="form-group">
        <%= text_field_tag :query, nil, class: "form-control", placeholder: "Search..." %>
      </div>
    <% end %>
  </div>
  <div id="test_search">
    <h4 class="active tab tab-control" id="test_name">Test Name</h4>
    <ul class="active tab tab-panel">
      <% ('A'..'Z').each do |letter| %>
        <li>
          <%= link_to letter, starts_with_search_tests_path(query: letter)  %>
        </li>
      <% end %>
      <li>
        <%= link_to "#", starts_with_search_tests_path(query: "#")  %>
      </li>
    </ul>
  </div>
</div>
<%= link_to pages_landing_path, class: 'btn btn-default pull-right' do %>
  <span class="glyphicon glyphicon-list-alt"></span>
  Back
<% end %>
  <%= link_to edit_test_path(@test), class: 'btn btn-primary pull-right' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <i class="fa fa-3x fa-bookmark-o pull-right" aria-hidden="true"></i>
  <h1>Show test</h1>


<dl class="dl-horizontal col-md-10">
  <dt>Procedure identifier:</dt>
  <dd><%= @test.procedure_identifier %></dd>

  <dt>Procedure name:</dt>
  <dd><%= @test.procedure_name %></dd>

  <dt>Procedure master number:</dt>
  <dd><%= @test.procedure_master_number %></dd>

  <dt>Synonyms:</dt>
  <dd><%= @test.synonyms %></dd>

  <dt>Test code:</dt>
  <dd><%= @test.test_code %></dd>

  <dt>Category code:</dt>
  <dd><%= @test.category_code %></dd>

  <dt>Category name:</dt>
  <dd><%= @test.category_name %></dd>

  <dt>Eppicare inactive flag:</dt>
  <dd><%= @test.eppicare_inactive_flag %></dd>

  <dt>Orderable:</dt>
  <dd><%= @test.orderable %></dd>

  <dt>Collection identifier:</dt>
  <dd><%= @test.collection_identifier %></dd>

  <dt>Collection name:</dt>
  <dd><%= @test.collection_name %></dd>

  <dt>Collection instructions:</dt>
  <dd><%= @test.collection_instructions %></dd>

  <dt>Process identifier:</dt>
  <dd><%= @test.process_identifier %></dd>

  <dt>Process name:</dt>
  <dd><%= @test.process_name %></dd>

  <dt>Process intructions:</dt>
  <dd><%= @test.process_intructions %></dd>

</dl>
